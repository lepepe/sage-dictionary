<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="pageTemplate.xsl"?>
<page>
<pagehead>
<generator>Sage Accpac SDK 6.8A</generator>
<keywords>PM PM0200 PMROGEN</keywords>
<pagetitle>Generate Reopen Worksheet</pagetitle>
<pagedate>2020-Nov-06</pagedate>
</pagehead>
<pagebody>
<header />
<view>
<title>Generate Reopen Worksheet</title>
<rotoid>PM0200</rotoid>
<protocollist>
<protocol>Superview</protocol></protocollist>
<dllname>PMROGEN</dllname>
<keylist>
</keylist>
<fieldlist>
<field>
<fieldname>WORKID</fieldname>
<fieldindex>1</fieldindex>
<fieldtype>String*30</fieldtype>
<fielddesc>Worksheet Number</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
<mask>%-16C</mask></field>
<field>
<fieldname>DESC</fieldname>
<fieldindex>2</fieldindex>
<fieldtype>String*60</fieldtype>
<fielddesc>Description</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
</field>
<field>
<fieldname>TRANSDATE</fieldname>
<fieldindex>3</fieldindex>
<fieldtype>Date</fieldtype>
<fielddesc>Worksheet Date</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
</field>
<field>
<fieldname>FISCALYEAR</fieldname>
<fieldindex>4</fieldindex>
<fieldtype>String*4</fieldtype>
<fielddesc>Fiscal Year</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
<mask>%04D</mask></field>
<field>
<fieldname>FISCALPER</fieldname>
<fieldindex>5</fieldindex>
<fieldtype>Integer</fieldtype>
<fielddesc>Fiscal Period</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
<fieldpresentlist><fieldpresent index="1" value="1" /><fieldpresent index="2" value="2" /><fieldpresent index="3" value="3" /><fieldpresent index="4" value="4" /><fieldpresent index="5" value="5" /><fieldpresent index="6" value="6" /><fieldpresent index="7" value="7" /><fieldpresent index="8" value="8" /><fieldpresent index="9" value="9" /><fieldpresent index="10" value="10" /><fieldpresent index="11" value="11" /><fieldpresent index="12" value="12" /><fieldpresent index="13" value="13" /></fieldpresentlist></field>
<field>
<fieldname>FMTCONTNO</fieldname>
<fieldindex>6</fieldindex>
<fieldtype>String*16</fieldtype>
<fielddesc>^1</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
<mask>%-16C</mask></field>
<field>
<fieldname>CONTRACT</fieldname>
<fieldindex>7</fieldindex>
<fieldtype>String*16</fieldtype>
<fielddesc>^1</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
<attributeflag type="Attribute" value="C" desc="Calculated" />
</attributeflaglist>
<mask>%-16C</mask></field>
<field>
<fieldname>PROJFROM</fieldname>
<fieldindex>8</fieldindex>
<fieldtype>String*16</fieldtype>
<fielddesc>From ^2</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
<mask>%-16N</mask></field>
<field>
<fieldname>PROJTO</fieldname>
<fieldindex>9</fieldindex>
<fieldtype>String*16</fieldtype>
<fielddesc>To</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
<mask>%-16N</mask></field>
<field>
<fieldname>REVERSE</fieldname>
<fieldindex>10</fieldindex>
<fieldtype>Integer</fieldtype>
<fielddesc>Reverse Entries for Completed Project Account Method ^5</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
</field>
<field>
<fieldname>BMETER</fieldname>
<fieldindex>11</fieldindex>
<fieldtype>Integer</fieldtype>
<fielddesc>Show progress bar during posting</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
<fieldpresentlist><fieldpresent index="0" value="No meter" /><fieldpresent index="1" value="Meter" /><fieldpresent index="2" value="Meter but no cancel button" /></fieldpresentlist></field>
<field>
<fieldname>DATEBUS</fieldname>
<fieldindex>12</fieldindex>
<fieldtype>Date</fieldtype>
<fielddesc>Posting Date</fielddesc>
<attributeflaglist>
<attributeflag type="Attribute" value="E" desc="Editable" />
<attributeflag type="Attribute" value="A" desc="Enabled" />
</attributeflaglist>
</field>
</fieldlist>
</view>
<footer />
